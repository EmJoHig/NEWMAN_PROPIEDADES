    <section id="contact" class="contact-area pt-200 pb-30 gray-bg">
        <div class="container"> 
            <div class="row">
                <div class="col-lg-6">
                    <div class="section-title" style="float: left;">
                        <h4 class="title">Contáctenos</h4>
                    </div> 
                    <div class="contact-form pt-30">
                        <form id="contact-form-contacto" action="/send-email-contact" method="POST">
                            <div class="single-form">
                                <input id="valueNombre" type="text" name="name" placeholder="Nombre" value="{{name}}">
                            </div> <!-- single form -->
                            <div class="single-form">
                                <input id="valueMail" type="email" name="email" placeholder="Email" value="{{email}}">
                            </div> <!-- single form -->
                            <div class="single-form">
                                <textarea id="valueMessage" name="message" placeholder="Mensaje" value="{{message}}"></textarea>
                            </div> <!-- single form -->

                            {{!-- ERRORES --}}
                            <br/>
                            {{#each errors}}
                                <div class="alert alert-danger alert-dismissible fade show" role="alert">
                                    {{text}}
                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                        <span aria-hidden="true">&times;</span> 
                                    </button>
                                </div>
                            {{/each}}

                            <div class="single-form">
                                <button class="main-btn" type="submit">Enviar Mensaje</button>
                            </div> <!-- single form -->
                        </form>
                    </div> <!-- contact form -->
                    <br/>
                    {{> messages}}
                </div>
                <div class="col-lg-6">
                    <div class="contact-map mt-60">
                        <div class="section-title">
                            <h4 class="title">Información de Contacto</h4>
                            <h2 class="title">Telefono: <span style="font-size: 90%; color:#838181">2215937093</span></h2>
                            <h2 class="title">Dirección: 
                                <span style="font-size: 90%; color:#838181">
                                    Calle 68 Nº 1106 e/ 55 y 56 La Plata
                                </span>
                            </h2>
                            {{!-- <p><span>Direccion:</span> Calle 2 Nº 1106 (e/55 y 56) La Plata (CP 1900)</p> --}}
                            {{!-- <h2 class="title">Horarios: <span style="font-size: 80%; color:#838181">2215937093</span></h2> --}}
                            <h1 class="title">Email: <span style="font-size: 90%;color:#838181;">administracion@inmobiliarianewman.com</span></h1>
                        </div> 
                    </div> <!-- contact map -->
                </div>
            </div> <!-- row -->
        </div> <!-- container -->
    </section>
    

