            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <nav class="navbar navbar-expand-lg">
                            <a class="navbar-brand" href="/">
                                <img src="/images/newman.png" alt="Logo">
                            </a> <!-- Logo -->
                            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                                <span class="toggler-icon"></span>
                                <span class="toggler-icon"></span>
                                <span class="toggler-icon"></span>
                            </button>

                            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                                <ul class="navbar-nav ml-auto">
                                    <li class="nav-item"><a class="page-scroll" href="/">INICIO</a></li>
                                    {{!-- <li class="nav-item"><a class="page-scroll" href="/alquiler">ALQUILER</a></li> --}}
                                    <li class="nav-item"><a class="page-scroll" href="/compra">COMPRA</a></li>
                                    <li class="nav-item"><a class="page-scroll" href="/compra-campos">CAMPOS</a></li>
                                    {{!-- <li class="nav-item"><a class="page-scroll" href="/venta">VENTA</a></li> --}}
                                    <li class="nav-item active"><a class="page-scroll" href="/contacto">CONTACTO</a></li>
                                </ul>
                            </div> <!-- navbar collapse -->
                        </nav> <!-- navbar -->
                    </div>
                </div> <!-- row -->
            </div> <!-- container -->
        </div> <!-- navigation -->
    </header>


    {{!-- {{#each errors}}
